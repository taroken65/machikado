<h2>ユーザー編集</h2>
<%= form_with model: @user, url:admin_user_path, local:true do |f| %>



  <h4>ニックネーム</h4>
  <%= f.text_field :name %>

  <h4>メールアドレス</h4>
  <%= f.email_field :email %>

  <h4>ステータス</h4>
　<%= f.radio_button :is_deleted, false %>
  <%= f.label :is_deleted, "有効" %>
  <%= f.radio_button :is_deleted, true %>
  <%= f.label :is_deleted, "退会" %>



  <%= f.submit '編集' %>
<% end %>