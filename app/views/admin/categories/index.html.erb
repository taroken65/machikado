<h2>カテゴリー追加</h2>
     <%= form_with model: @category, url: admin_categories_path, local: true do |f| %>
      ジャンル名
      <%= f.text_field :name, :placeholder => "カテゴリー名" %>
      <%= f.submit '新規投稿' %>
     <% end %>

<h2>カテゴリー一覧</h2>
<table class="table">
      <thead>
       <tr>
        <th>カテゴリー</th>
      </tr>
      </thead>
      <% @categories.each do |f| %>
      <tr>
       <td><%= f.name %></td>
       <% end %>
      <tr>
      </table>