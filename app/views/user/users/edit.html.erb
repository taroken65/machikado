<div class="container mt-5">
<h2>マイページ編集</h2>
<%= form_with model: @user, url:user_path,local:true do |f| %>

  <h4>プロフィール画像</h4>
  <%= f.attachment_field :profile_image %>

  <h4>ニックネーム</h4>
  <%= f.text_field :name %>

  <h4>メールアドレス</h4>
  <%= f.email_field :email %>

  <%= f.submit '編集' %>
<% end %>
</div>